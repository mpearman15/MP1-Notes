{
  "manifest_version": 3,
  "name": "notes",
  "description": "notes extension!",
  "version": "1.0",
  "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
        "matches": [
            "*://*/*"
        ],
        "js": [
            "content.js"
        ]
    }
]
}